generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  body String
  category String
  link String?
  nsfw Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

   @@map("posts")
}